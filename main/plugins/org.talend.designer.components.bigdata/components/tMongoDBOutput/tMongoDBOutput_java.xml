<COMPONENT>
    <HEADER
        PLATEFORM="ALL"
        SERIAL=""
        VERSION="0.101"
        STATUS="ALPHA"
        COMPATIBILITY="ALL"
        AUTHOR="Talend"
        SCHEMA_AUTO_PROPAGATE="true"
        DATA_AUTO_PROPAGATE="false"
        EXTENSION="org.talend.designer.fileoutputxml"
        RELEASE_DATE="20070210A"
        STARTABLE="false"
        PARTITIONING="NONE"
        LOG4J_ENABLED="true"
    >
        <SIGNATURE/>
    </HEADER>
    <FAMILIES>
        <FAMILY>Databases NoSQL/MongoDB</FAMILY>
        <FAMILY>Big Data/MongoDB</FAMILY>
    </FAMILIES>
    <DOCUMENTATION>
        <URL />
    </DOCUMENTATION>
    <CONNECTORS>
        <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="0"/>
        <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0"/>
        <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
        <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
        <CONNECTOR CTYPE="COMPONENT_OK"/>
        <CONNECTOR CTYPE="COMPONENT_ERROR"/>
        <CONNECTOR CTYPE="RUN_IF"/>
    </CONNECTORS>
    <PARAMETERS>
        <PARAMETER NAME="PROPERTY" FIELD="PROPERTY_TYPE" NUM_ROW="1" REPOSITORY_VALUE="NOSQL:MONGODB" SHOW_IF="USE_EXISTING_CONNECTION == 'false'"/>
        <PARAMETER
            NAME="USE_EXISTING_CONNECTION"
            FIELD="CHECK"
            NUM_ROW="2"
            GROUP="CONNECTION"
        >
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        <PARAMETER
            NAME="CONNECTION"
            FIELD="COMPONENT_LIST"
            REQUIRED="true" FILTER="tMongoDBConnection"
            NUM_ROW="2"
            SHOW_IF="USE_EXISTING_CONNECTION == 'true'"
            GROUP="CONNECTION"
        />
        <PARAMETER
            NAME="DB_VERSION"
            FIELD="CLOSED_LIST"
            REPOSITORY_VALUE="DB_VERSION"
            NUM_ROW="3"
            SHOW_IF="(USE_EXISTING_CONNECTION == 'false')"
            GROUP="CONNECTION">
            <ITEMS DEFAULT="MONGODB_4_4_X">
                <ITEM NAME="MONGODB_3_5_X" VALUE="MONGODB_3_5_X"/>
                <ITEM NAME="MONGODB_4_4_X" VALUE="MONGODB_4_4_X"/>
            </ITEMS>
        </PARAMETER>
        
        <PARAMETER FIELD="CHECK" NAME="USE_CONNECTION_STRING" REPOSITORY_VALUE="USE_CONNECTION_STRING" NUM_ROW="4" SHOW_IF="(USE_EXISTING_CONNECTION == 'false') AND (DB_VERSION=='MONGODB_4_4_X')" GROUP="CONNECTION">
            <DEFAULT>"false"</DEFAULT>
        </PARAMETER>
        
        <PARAMETER FIELD="TEXT" NAME="CONNECTION_STRING" REPOSITORY_VALUE="CONNECTION_STRING" NUM_ROW="4" REQUIRED="true" SHOW_IF="isShow[USE_CONNECTION_STRING] AND (USE_CONNECTION_STRING=='true')" GROUP="CONNECTION">
            <DEFAULT>""</DEFAULT>
        </PARAMETER>
        
        <PARAMETER
            FIELD="CHECK"
            NAME="USE_REPLICA_SET"
            REPOSITORY_VALUE="USE_REPLICA_SET"
            NUM_ROW="5"
            SHOW_IF="(USE_EXISTING_CONNECTION == 'false') AND (((DB_VERSION=='MONGODB_4_4_X') AND (USE_CONNECTION_STRING=='false')) OR (DB_VERSION=='MONGODB_3_5_X'))"
            GROUP="CONNECTION">
            <DEFAULT>"false"</DEFAULT>
        </PARAMETER>
        <PARAMETER
            NAME="REPLICA_SET"
            FIELD="TABLE"
            REPOSITORY_VALUE="REPLICA_SET"
            REQUIRED_IF="(USE_REPLICA_SET=='true')"
            NUM_ROW="6"
            NB_LINES="5"
            SHOW_IF="isShow[USE_REPLICA_SET] AND (USE_REPLICA_SET=='true')"
            GROUP="CONNECTION">
            <ITEMS>
                <ITEM NAME="REPLICA_HOST" FIELD="TEXT" />
                <ITEM NAME="REPLICA_PORT" FIELD="TEXT" />
            </ITEMS>
        </PARAMETER>
        <PARAMETER
            FIELD="TEXT"
            NAME="HOST"
            REPOSITORY_VALUE="HOST"
            NUM_ROW="7"
            REQUIRED="true"
            SHOW_IF="isShow[USE_REPLICA_SET] AND (USE_REPLICA_SET=='false')"
            GROUP="CONNECTION"
        >
            <DEFAULT>""</DEFAULT>
        </PARAMETER>
        <PARAMETER
            FIELD="TEXT"
            NAME="PORT"
            REPOSITORY_VALUE="PORT"
            NUM_ROW="7"
            SHOW_IF="isShow[HOST]"
            GROUP="CONNECTION"
        >
            <DEFAULT>27017</DEFAULT>
        </PARAMETER>
        
        <PARAMETER
            FIELD="TEXT"
            NAME="DATABASE"
            REPOSITORY_VALUE="DATABASE"
            NUM_ROW="9"
            REQUIRED="true"
            SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
            GROUP="CONNECTION"
        >
            <DEFAULT>""</DEFAULT>
        </PARAMETER>

        <PARAMETER FIELD="CHECK" NAME="USE_SSL" REPOSITORY_VALUE="USE_SSL" NUM_ROW="10" SHOW_IF="(USE_EXISTING_CONNECTION == 'false')" GROUP="CONNECTION">
            <DEFAULT>"false"</DEFAULT>
        </PARAMETER>

        <PARAMETER FIELD="CHECK" NAME="SET_WRITE_CONCERN" NUM_ROW="11" GROUP="WRITE_CONCERN_GROUP"
            NOT_SHOW_IF="((USE_EXISTING_CONNECTION == 'true') AND (#LINK@NODE.CONNECTION.DB_VERSION== 'MONGODB_4_4_X'))">
            <DEFAULT>"false"</DEFAULT>
        </PARAMETER>

        <PARAMETER NAME="WRITE_CONCERN" FIELD="CLOSED_LIST" NUM_ROW="17" GROUP="WRITE_CONCERN_GROUP"
            SHOW_IF="(((USE_EXISTING_CONNECTION == 'false') AND (SET_WRITE_CONCERN=='true')) OR ((SET_WRITE_CONCERN=='true') AND (USE_EXISTING_CONNECTION == 'true') AND (#LINK@NODE.CONNECTION.DB_VERSION != 'MONGODB_4_4_X')))">
            <ITEMS DEFAULT="ACKNOWLEDGED">
                <ITEM NAME="ACKNOWLEDGED" VALUE="ACKNOWLEDGED"/>
                <ITEM NAME="UNACKNOWLEDGED" VALUE="UNACKNOWLEDGED"/>
                <ITEM NAME="JOURNALED" VALUE="JOURNALED"/>
                <ITEM NAME="REPLICA_ACKNOWLEDGED" VALUE="REPLICA_ACKNOWLEDGED" NOT_SHOW_IF="(((USE_EXISTING_CONNECTION == 'false') AND (DB_VERSION == 'MONGODB_4_4_X')) OR ((USE_EXISTING_CONNECTION == 'true') AND (#LINK@NODE.CONNECTION.DB_VERSION == 'MONGODB_4_4_X')))"/>
                <ITEM NAME="MAJORITY" VALUE="MAJORITY" SHOW_IF="(((USE_EXISTING_CONNECTION == 'false') AND (DB_VERSION == 'MONGODB_4_4_X')) OR ((USE_EXISTING_CONNECTION == 'true') AND (#LINK@NODE.CONNECTION.DB_VERSION == 'MONGODB_4_4_X')))"/>
                <ITEM NAME="W1" VALUE="W1" SHOW_IF="(((USE_EXISTING_CONNECTION == 'false') AND (DB_VERSION == 'MONGODB_4_4_X')) OR ((USE_EXISTING_CONNECTION == 'true') AND (#LINK@NODE.CONNECTION.DB_VERSION == 'MONGODB_4_4_X')))"/>
                <ITEM NAME="W2" VALUE="W2" SHOW_IF="(((USE_EXISTING_CONNECTION == 'false') AND (DB_VERSION == 'MONGODB_4_4_X')) OR ((USE_EXISTING_CONNECTION == 'true') AND (#LINK@NODE.CONNECTION.DB_VERSION == 'MONGODB_4_4_X')))"/>
                <ITEM NAME="W3" VALUE="W3" SHOW_IF="(((USE_EXISTING_CONNECTION == 'false') AND (DB_VERSION == 'MONGODB_4_4_X')) OR ((USE_EXISTING_CONNECTION == 'true') AND (#LINK@NODE.CONNECTION.DB_VERSION == 'MONGODB_4_4_X')))"/>
            </ITEMS>
        </PARAMETER>

        <PARAMETER NAME="WRITE_CONCERN_NOTE" FIELD="LABEL" NUM_ROW="18" GROUP="WRITE_CONCERN_GROUP"
            SHOW_IF="((USE_EXISTING_CONNECTION == 'true') AND (#LINK@NODE.CONNECTION.DB_VERSION== 'MONGODB_4_4_X'))">
            <DEFAULT>Note : Use Connection component to set Write Concern</DEFAULT>
        </PARAMETER>

        <PARAMETER FIELD="CHECK" NAME="SET_BULK_WRITE" NUM_ROW="19" NOT_SHOW_IF="(DB_VERSION=='MONGODB_2_5_X') OR ((USE_EXISTING_CONNECTION == 'true') AND (#LINK@NODE.CONNECTION.DB_VERSION=='MONGODB_2_5_X'))" GROUP="BULK_WRITE_GROUP">
            <DEFAULT>"false"</DEFAULT>
        </PARAMETER>

        <PARAMETER NAME="BULK_WRITE_TYPE" FIELD="CLOSED_LIST" NUM_ROW="20" NOT_SHOW_IF="(SET_BULK_WRITE=='false') OR ((DB_VERSION=='MONGODB_2_5_X') OR ((USE_EXISTING_CONNECTION == 'true') AND (#LINK@NODE.CONNECTION.DB_VERSION=='MONGODB_2_5_X')))" GROUP="BULK_WRITE_GROUP">
            <ITEMS DEFAULT="UNORDERED">
                <ITEM NAME="UNORDERED" VALUE="Unordered"/>
                <ITEM NAME="ORDERED" VALUE="Ordered"/>
            </ITEMS>
        </PARAMETER>

        <PARAMETER FIELD="TEXT" NAME="BULK_WRITE_SIZE" REPOSITORY_VALUE="BULK_WRITE_SIZE" NUM_ROW="21" NOT_SHOW_IF="(SET_BULK_WRITE=='false') OR ((DB_VERSION=='MONGODB_2_5_X') OR ((USE_EXISTING_CONNECTION == 'true') AND (#LINK@NODE.CONNECTION.DB_VERSION=='MONGODB_2_5_X')))" GROUP="BULK_WRITE_GROUP">
            <DEFAULT>"1000"</DEFAULT>
        </PARAMETER>


        <PARAMETER
            NAME="REQUIRED_AUTHENTICATION"
            FIELD="CHECK"
            REPOSITORY_VALUE="REQUIRED_AUTHENTICATION"
            NUM_ROW="22"
            SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
            GROUP="AUTHENTICATION"
        >
            <DEFAULT>false</DEFAULT>
        </PARAMETER>

        <PARAMETER NAME="AUTHENTICATION_MECHANISM" REPOSITORY_VALUE="AUTHENTICATION_MECHANISM" FIELD="CLOSED_LIST" NUM_ROW="23"
                SHOW_IF="(USE_EXISTING_CONNECTION == 'false') AND (REQUIRED_AUTHENTICATION == 'true')" GROUP="AUTHENTICATION">
            <ITEMS DEFAULT="SCRAMSHA1_MEC">
                <ITEM NAME="NEGOTIATE_MEC" VALUE="NEGOTIATE_MEC" SHOW_IF="(DB_VERSION!='INVALID')"/>
                <ITEM NAME="PLAIN_MEC" VALUE="PLAIN_MEC" SHOW_IF="(DB_VERSION!='MONGODB_2_5_X')"/>
                <ITEM NAME="SCRAMSHA1_MEC" VALUE="SCRAMSHA1_MEC" SHOW_IF="(DB_VERSION=='MONGODB_3_0_X') OR (DB_VERSION=='MONGODB_3_2_X') OR (DB_VERSION=='MONGODB_3_5_X')" />
                <ITEM NAME="SCRAMSHA256_MEC" VALUE="SCRAMSHA256_MEC" SHOW_IF="(DB_VERSION=='MONGODB_4_4_X')" />
                <ITEM NAME="KERBEROS_MEC" VALUE="KERBEROS_MEC" SHOW_IF="(DB_VERSION!='INVALID')"/>
                <ITEM NAME="X509" VALUE="X509" />
            </ITEMS>
        </PARAMETER>

        <PARAMETER FIELD="CHECK" NAME="SET_AUTHENTICATION_DATABASE" REPOSITORY_VALUE="SET_AUTHENTICATION_DATABASE"
            NUM_ROW="24" SHOW_IF="((AUTHENTICATION_MECHANISM=='NEGOTIATE_MEC') OR (AUTHENTICATION_MECHANISM=='SCRAMSHA1_MEC') OR (AUTHENTICATION_MECHANISM=='SCRAMSHA256_MEC')) AND (REQUIRED_AUTHENTICATION == 'true') AND (USE_EXISTING_CONNECTION == 'false')" GROUP="AUTHENTICATION">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>

        <PARAMETER FIELD="TEXT" NAME="AUTHENTICATION_DATABASE" REPOSITORY_VALUE="AUTHENTICATION_DATABASE" NUM_ROW="25"
            SHOW_IF="(SET_AUTHENTICATION_DATABASE=='true') AND (isShow[SET_AUTHENTICATION_DATABASE])" GROUP="AUTHENTICATION">
            <DEFAULT>""</DEFAULT>
        </PARAMETER>
        
        <PARAMETER FIELD="CHECK" NAME="SET_X509_USERNAME" NUM_ROW="26" SHOW_IF="(AUTHENTICATION_MECHANISM=='X509') AND (REQUIRED_AUTHENTICATION == 'true') AND (USE_EXISTING_CONNECTION == 'false')" GROUP="AUTHENTICATION">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>

        <PARAMETER
            FIELD="TEXT"
            NAME="USERNAME"
            REPOSITORY_VALUE="USERNAME"
            NUM_ROW="26"
            SHOW_IF="(((AUTHENTICATION_MECHANISM=='X509') AND (SET_X509_USERNAME=='true')) OR (AUTHENTICATION_MECHANISM=='NEGOTIATE_MEC') OR (AUTHENTICATION_MECHANISM=='PLAIN_MEC') OR (AUTHENTICATION_MECHANISM=='SCRAMSHA1_MEC') OR (AUTHENTICATION_MECHANISM=='SCRAMSHA256_MEC')) AND (REQUIRED_AUTHENTICATION == 'true') AND (USE_EXISTING_CONNECTION == 'false')"
            GROUP="AUTHENTICATION"
        >
            <DEFAULT>""</DEFAULT>
        </PARAMETER>
        <PARAMETER
            FIELD="PASSWORD"
            NAME="PASSWORD"
            REPOSITORY_VALUE="PASSWORD"
            NUM_ROW="26"
            SHOW_IF="((AUTHENTICATION_MECHANISM=='NEGOTIATE_MEC') OR (AUTHENTICATION_MECHANISM=='PLAIN_MEC') OR (AUTHENTICATION_MECHANISM=='SCRAMSHA1_MEC') OR (AUTHENTICATION_MECHANISM=='SCRAMSHA256_MEC')) AND (REQUIRED_AUTHENTICATION == 'true') AND (USE_EXISTING_CONNECTION == 'false')"
            GROUP="AUTHENTICATION"
        >
            <DEFAULT>""</DEFAULT>
        </PARAMETER>

        <PARAMETER FIELD="TEXT" NAME="KRB_USER_PRINCIPAL" REPOSITORY_VALUE="KRB_USER_PRINCIPAL" NUM_ROW="27"
            SHOW_IF="(AUTHENTICATION_MECHANISM=='KERBEROS_MEC') AND (REQUIRED_AUTHENTICATION=='true') AND (USE_EXISTING_CONNECTION == 'false')" GROUP="AUTHENTICATION">
            <DEFAULT>"mongouser@EXAMPLE.COM"</DEFAULT>
        </PARAMETER>

        <PARAMETER FIELD="TEXT" NAME="KRB_REALM" REPOSITORY_VALUE="KRB_REALM" NUM_ROW="28"
            SHOW_IF="(AUTHENTICATION_MECHANISM=='KERBEROS_MEC') AND (REQUIRED_AUTHENTICATION=='true') AND (USE_EXISTING_CONNECTION == 'false')" GROUP="AUTHENTICATION">
            <DEFAULT>"EXAMPLE.COM"</DEFAULT>
        </PARAMETER>

        <PARAMETER FIELD="TEXT" NAME="KRB_KDC" REPOSITORY_VALUE="KRB_KDC" NUM_ROW="29"
            SHOW_IF="(AUTHENTICATION_MECHANISM=='KERBEROS_MEC') AND (REQUIRED_AUTHENTICATION=='true') AND (USE_EXISTING_CONNECTION == 'false')" GROUP="AUTHENTICATION">
            <DEFAULT>"kdc.example.com"</DEFAULT>
        </PARAMETER>

        <PARAMETER
            FIELD="TEXT"
            NAME="COLLECTION"
            REQUIRED="true"
            NUM_ROW="30"
        >
            <DEFAULT>""</DEFAULT>
        </PARAMETER>
        <PARAMETER
            NAME="DROP_COLLECTION_CREATE"
            FIELD="CHECK"
            NUM_ROW="35"
        >
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        <PARAMETER
            NAME="DATA_ACTION"
          FIELD="CLOSED_LIST"
          NUM_ROW="40"
        >
            <ITEMS DEFAULT="INSERT">
                <ITEM NAME="INSERT" VALUE="INSERT" />
                <ITEM NAME="UPDATE" VALUE="UPDATE"/>
                <ITEM NAME="SET" VALUE="SET"/>
                <ITEM NAME="UPSERT" VALUE="UPSERT"/>
                <ITEM NAME="UPSERT_WITH_SET" VALUE="UPSERT_WITH_SET"/>
                <ITEM NAME="DELETE" VALUE="DELETE"/>
            </ITEMS>
        </PARAMETER>
        <PARAMETER
            NAME="UPDATE_ALL"
            FIELD="CHECK"
            NUM_ROW="40"
            SHOW_IF="((DATA_ACTION=='SET') OR (DATA_ACTION=='UPSERT_WITH_SET')) AND (IS_VIRTUAL_COMPONENT == 'false')">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        <PARAMETER
            NAME="DELETE_ALL"
            FIELD="CHECK"
            NUM_ROW="40"
            SHOW_IF="((DATA_ACTION=='DELETE') AND ((IS_VIRTUAL_COMPONENT == 'false') AND (((DB_VERSION=='MONGODB_3_5_X') OR (DB_VERSION=='MONGODB_4_4_X')))))">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        <PARAMETER
            NAME="SCHEMA_FLOW"
            FIELD="SCHEMA_TYPE"
            NUM_ROW="45"
        >
            <DEFAULT>""</DEFAULT>
        </PARAMETER>
        <PARAMETER
            NAME="MAPPING"
            FIELD="TABLE"
            REQUIRED="true"
            NUM_ROW="46"
            NB_LINES="5"
            SHOW_IF="(IS_VIRTUAL_COMPONENT == 'false')"
        >
            <ITEMS BASED_ON_SCHEMA="true">
                <ITEM NAME="PARENT_NODE_PATH" FIELD="TEXT" />
                <ITEM NAME="REMOVE_NULL_FIELD" FIELD="CHECK" VALUE="false"/>
                <ITEM NAME="UPDATABLE" VALUE="true" FIELD="CHECK" SHOW_IF="(((USE_EXISTING_CONNECTION == 'false') AND ((DB_VERSION=='MONGODB_3_5_X') OR (DB_VERSION=='MONGODB_4_4_X'))) OR ((USE_EXISTING_CONNECTION == 'true') AND ((#LINK@NODE.CONNECTION.DB_VERSION=='MONGODB_3_5_X') OR (#LINK@NODE.CONNECTION.DB_VERSION=='MONGODB_4_4_X')))) AND (DATA_ACTION=='UPSERT_WITH_SET')"/>
                <ITEM NAME="INSERTABLE" VALUE="true" FIELD="CHECK" SHOW_IF="(((USE_EXISTING_CONNECTION == 'false') AND ((DB_VERSION=='MONGODB_3_5_X') OR (DB_VERSION=='MONGODB_4_4_X'))) OR ((USE_EXISTING_CONNECTION == 'true') AND ((#LINK@NODE.CONNECTION.DB_VERSION=='MONGODB_3_5_X') OR (#LINK@NODE.CONNECTION.DB_VERSION=='MONGODB_4_4_X')))) AND (DATA_ACTION=='UPSERT_WITH_SET')"/>
            </ITEMS>
        </PARAMETER>
        <PARAMETER
            NAME="DIE_ON_ERROR"
            FIELD="CHECK"
            NUM_ROW="50">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
    </PARAMETERS>
    <ADVANCED_PARAMETERS>
        <PARAMETER FIELD="CHECK" NAME="QUERYOPTION_NOTIMEOUT" REPOSITORY_VALUE="QUERYOPTION_NOTIMEOUT"
            NUM_ROW="1" GROUP="ADVANCED_QUERY_OPTIONS" SHOW_IF="((USE_EXISTING_CONNECTION == 'false') AND (DB_VERSION != 'MONGODB_4_4_X'))">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>

        <PARAMETER
            NAME="IS_VIRTUAL_COMPONENT"
            FIELD="CHECK"
            REQUIRED="true"
            NUM_ROW="5"
        >
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        

        
        
        <PARAMETER
            NAME="ROOT"
            FIELD="TABLE"
            REQUIRED_IF="(IS_VIRTUAL_COMPONENT == 'true')"
            NUM_ROW="10"
            NB_LINES="3"
            SHOW="false"
        >
            <ITEMS>
                <ITEM NAME="PATH" FIELD="TEXT" />
                <ITEM NAME="COLUMN" FIELD="TEXT" />
                <ITEM NAME="VALUE" FIELD="TEXT" />
                <ITEM NAME="ATTRIBUTE" FIELD="TEXT" />
                <ITEM NAME="ORDER" FIELD="TEXT" />
            </ITEMS>
        </PARAMETER>

        <PARAMETER
            NAME="GROUP"
            FIELD="TABLE"
            REQUIRED="false"
            NUM_ROW="20"
            NB_LINES="5"
            SHOW="false"
        >
            <ITEMS>
                <ITEM NAME="PATH" FIELD="TEXT" />
                <ITEM NAME="COLUMN" FIELD="TEXT" />
                <ITEM NAME="VALUE" FIELD="TEXT" />
                <ITEM NAME="ATTRIBUTE" FIELD="TEXT" />
                <ITEM NAME="ORDER" FIELD="TEXT" />
            </ITEMS>
        </PARAMETER>

        <PARAMETER
            NAME="LOOP"
            FIELD="TABLE"
            REQUIRED_IF="(IS_VIRTUAL_COMPONENT == 'true')"
            NUM_ROW="30"
            SHOW="false"
        >
            <ITEMS>
                <ITEM NAME="PATH" FIELD="TEXT" />
                <ITEM NAME="COLUMN" FIELD="TEXT" />
                <ITEM NAME="VALUE" FIELD="TEXT" />
                <ITEM NAME="ATTRIBUTE" FIELD="TEXT" />
                <ITEM NAME="ORDER" FIELD="TEXT" />
            </ITEMS>
        </PARAMETER>

        <PARAMETER
            NAME="MAP"
            FIELD="EXTERNAL"
            NUM_ROW="40"
            SHOW_IF="(IS_VIRTUAL_COMPONENT == 'true')"
            GROUP="JSON_CONFIGURATION"
        >
            <DEFAULT />
        </PARAMETER>

        <PARAMETER
            NAME="CREATE_EMPTY_ELEMENT"
            FIELD="CHECK"
            NUM_ROW="50"
            SHOW_IF="(IS_VIRTUAL_COMPONENT == 'true')"
            GROUP="JSON_CONFIGURATION"
        >
            <DEFAULT>true</DEFAULT>
        </PARAMETER>

        <PARAMETER NAME="IGNORE_SERVICE_ATTRIBUTES_FOR_EMPTY" FIELD="CHECK" NUM_ROW="50"
            GROUP="JSON_CONFIGURATION"
            SHOW_IF="(IS_VIRTUAL_COMPONENT == 'true') AND (CREATE_EMPTY_ELEMENT=='false')">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>

        <PARAMETER
            NAME="GROUPBYS"
            FIELD="TABLE"
            NUM_ROW="60"
            NB_LINES="3"
            SHOW_IF="(IS_VIRTUAL_COMPONENT == 'true')"
            GROUP="JSON_CONFIGURATION"
        >
            <ITEMS>
                <ITEM NAME="INPUT_COLUMN" FIELD="PREV_COLUMN_LIST" />
            </ITEMS>
        </PARAMETER>
        <PARAMETER
            NAME="REMOVE_ROOT"
            FIELD="CHECK"
            NUM_ROW="70"
            SHOW_IF="(IS_VIRTUAL_COMPONENT == 'true')"
            GROUP="JSON_CONFIGURATION"
        >
            <DEFAULT>true</DEFAULT>
        </PARAMETER>
        <PARAMETER
            FIELD="TEXT"
            NAME="DATA_NODE"
            REQUIRED_IF="(IS_VIRTUAL_COMPONENT == 'true')"
            NUM_ROW="75"
            SHOW_IF="(IS_VIRTUAL_COMPONENT == 'true') AND (DATA_ACTION !='INSERT') AND (DATA_ACTION !='DELETE')"
            GROUP="JSON_CONFIGURATION"
        >
            <DEFAULT>""</DEFAULT>
        </PARAMETER>
        <PARAMETER
            FIELD="TEXT"
            NAME="QUERY_NODE"
            REQUIRED_IF="(IS_VIRTUAL_COMPONENT == 'true')"
            NUM_ROW="75"
            SHOW_IF="(IS_VIRTUAL_COMPONENT == 'true') AND (DATA_ACTION !='INSERT') AND (DATA_ACTION !='DELETE')"
            GROUP="JSON_CONFIGURATION"
        >
            <DEFAULT>""</DEFAULT>
        </PARAMETER>
        
        <PARAMETER
            NAME="NODES"
            FIELD="TABLE"
            REQUIRED_IF="(IS_VIRTUAL_COMPONENT == 'true')"
            NUM_ROW="78"
            NB_LINES="5"
            SHOW_IF="(IS_VIRTUAL_COMPONENT == 'true') AND ((DATA_ACTION =='UPSERT_WITH_SET') OR (DATA_ACTION =='SET'))"
            GROUP="JSON_CONFIGURATION">
            <ITEMS>
                <ITEM NAME="NODE_TYPE" FIELD="OPENED_LIST">
                    <ITEMS DEFAULT="PUSH">
                        <ITEM NAME="PUSH" VALUE="push"/>
                        <ITEM NAME="INC" VALUE="inc"/>
                        <ITEM NAME="RENAME" VALUE="rename"/>
                        <ITEM NAME="UNSET" VALUE="unset"/>
                        <ITEM NAME="MIN" VALUE="min"/>
                        <ITEM NAME="MAX" VALUE="max"/>
                    </ITEMS>
                </ITEM>
                <ITEM NAME="NODE_REF" FIELD="TEXT" />
            </ITEMS>
        </PARAMETER>
        
        <PARAMETER
            NAME="COMPACT_FORMAT"
            FIELD="CHECK"
            REQUIRED_IF="(IS_VIRTUAL_COMPONENT == 'true')"
            NUM_ROW="80"
            SHOW="false"
            GROUP="JSON_CONFIGURATION"
        >
            <DEFAULT>true</DEFAULT>
        </PARAMETER>
    </ADVANCED_PARAMETERS>

    <CODEGENERATION>
        <TEMPLATES INPUT="In" OUTPUT="Out">
            <TEMPLATE NAME="In" COMPONENT="tWriteXMLFieldOut">
                <LINK_TO NAME="Out" CTYPE="COMPONENT_OK" />
            </TEMPLATE>

            <TEMPLATE NAME="Out" COMPONENT="tMongoDBWriteConf" />

            <TEMPLATE_PARAM SOURCE="self.GROUPBYS" TARGET="In.GROUPBYS" />

            <TEMPLATE_PARAM SOURCE="self.ROOT" TARGET="In.ROOT" />
            <TEMPLATE_PARAM SOURCE="self.GROUP" TARGET="In.GROUP" />
            <TEMPLATE_PARAM SOURCE="self.LOOP" TARGET="In.LOOP" />
            <TEMPLATE_PARAM SOURCE="self.COMPACT_FORMAT" TARGET="In.COMPACT_FORMAT" />
            <TEMPLATE_PARAM SOURCE="self.SCHEMA_FLOW" TARGET="In.SCHEMA" />
            <TEMPLATE_PARAM SOURCE="self.CREATE_EMPTY_ELEMENT" TARGET="In.CREATE_EMPTY_ELEMENT" />
            <TEMPLATE_PARAM SOURCE="self.IGNORE_SERVICE_ATTRIBUTES_FOR_EMPTY" TARGET="In.IGNORE_SERVICE_ATTRIBUTES_FOR_EMPTY" />

            <TEMPLATE_PARAM SOURCE="self.ROOT" TARGET="Out.ROOT" />
            <TEMPLATE_PARAM SOURCE="self.GROUP" TARGET="Out.GROUP" />
            <TEMPLATE_PARAM SOURCE="self.LOOP" TARGET="Out.LOOP" />
